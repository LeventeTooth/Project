<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Project.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Project"
    Shell.FlyoutBehavior="Disabled"
    Title="Project">

    <ShellContent
        Title="KaralkaTrack"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent
        Title="Manage Cars"
        ContentTemplate="{DataTemplate local:CarsPage}"
        Route="CarsPage"/>
    <ShellContent
        Title="Manage Tracks"
        ContentTemplate="{DataTemplate local:TracksPage}"
        Route="TracksPage"/>
    <ShellContent
        Title="Manage Rents"
        ContentTemplate="{DataTemplate local:RentsPage}"
        Route="RentsPage"/>

</Shell>
